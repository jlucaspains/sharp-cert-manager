		"eslint-plugin-svelte": "^2.46.0",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"svelte": "^5.1.9",
		"svelte-check": "^4.0.5",
		"tailwindcss": "^3.4.14",
